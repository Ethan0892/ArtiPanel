# ArtiPanel Frontend Configuration

# API Configuration
REACT_APP_API_URL=http://localhost:4000
REACT_APP_API_TIMEOUT=30000

# WebSocket Configuration
REACT_APP_WS_URL=ws://localhost:4000
REACT_APP_WS_RECONNECT_ATTEMPTS=5
REACT_APP_WS_RECONNECT_DELAY=3000

# Environment
REACT_APP_ENVIRONMENT=development
REACT_APP_VERSION=0.1.0-alpha.1

# Features
REACT_APP_FEATURE_GAMING_SERVERS=true
REACT_APP_FEATURE_NAS_MANAGEMENT=false
REACT_APP_FEATURE_KUBERNETES=false
REACT_APP_FEATURE_AUTO_SCALING=false
REACT_APP_FEATURE_AI_SUGGESTIONS=false

# Performance
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_TRACKING=true
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true
REACT_APP_CACHE_DURATION=300000

# Security
REACT_APP_ENABLE_CSP=true
REACT_APP_SESSION_TIMEOUT=3600000
REACT_APP_ENABLE_BIOMETRIC=false

# Debug
REACT_APP_DEBUG_MODE=false
REACT_APP_LOG_LEVEL=info
